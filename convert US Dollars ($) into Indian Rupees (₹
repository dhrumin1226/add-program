#include <stdio.h>

int main() {
    float dollar, rupees;
    float rate = 83.0; // 1 USD = 83 INR (you can change this)

    // Input amount in dollars
    printf("Enter amount in USD ($): ");
    scanf("%f", &dollar);

    // Convert to rupees
    rupees = dollar * rate;

    // Display result
    printf("%.2f USD = %.2f INR\n", dollar, rupees);

    return 0;
}